
#define an R function
square <- function(x) x*x

add_two_number <- function(x,y) x+y

longer_function <- function(a,b){
  both <- a + b
  both_squared <- square(both)
  both_squared #last line is always returned
}

sa <- read_tsv('raw_data/sampleA.txt')
sb <- read_tsv('raw_data/sampleB.txt')
merged_dataset <- full_join(sa,sb)
